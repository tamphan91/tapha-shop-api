{
  "name": "nest-temp",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.8.5",
    "@nestjs/core": "^6.8.5",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.8.5",
    "@nestjs/platform-socket.io": "^6.8.5",
    "@nestjs/platform-ws": "^6.8.5",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.1.3",
    "@nestjs/websockets": "^6.8.5",
    "@nestjsx/crud": "^4.2.0",
    "@nestjsx/crud-typeorm": "^4.2.0",
    "@types/node-schedule": "^1.2.4",
    "@types/puppeteer": "^1.20.2",
    "apollo-server-express": "^2.14.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "cloudinary": "^1.15.0",
    "csurf": "^1.10.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.0.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "helmet": "^3.20.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "node-schedule": "^1.3.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "puppeteer": "^1.20.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "socket.io-redis": "^5.2.0",
    "swagger-ui-express": "^4.0.7",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.6.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/hapi__joi": "^16.0.3",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.14.17",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.2",
    "jest": "^23.6.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
