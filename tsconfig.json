{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2018",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "allowSyntheticDefaultImports": true,
    // "esModuleInterop": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "./plugins/**/*",
    "./typings/**/*",
    "./built/**/*" // This is what fixed it!
  ]
}
